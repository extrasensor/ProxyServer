# Proxy Server Configuration
PORT=3000

# Optional: Roblox Authentication Cookie (for better API access)
# Get this from your browser after logging into Roblox
# WARNING: Keep this secret! Never share or commit to Git
ROBLOSECURITY=

# API Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=30

# Cache Settings
CACHE_TTL_SECONDS=10

# CORS Settings (Your Roblox game place)
ALLOWED_ORIGINS=*
